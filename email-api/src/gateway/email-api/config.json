{
    "configurations": [
        {
            "name": "life-dev",
            "policies": [
                {
                    "name": "SA.SpikeArrest.xml",
                    "tokens": [
                        {
                            "xpath": "/SpikeArrest/Rate",
                            "value": "30ps"
                        }
                    ]
                }
            ],
            "proxies": [
                {
                    "name": "default.xml",
                    "tokens": [
                        {
                            "xpath": "/ProxyEndpoint/HTTPProxyConnection/BasePath",
                            "value": "/email/v1/cm"
                        },
						{
                            "xpath": "/ProxyEndpoint/HTTPProxyConnection/VirtualHost",
                            "value": "default"
                        }
                    ]
                }
            ],
            "targets": [
                {
                    "name": "default.xml",
                    "tokens": [
                        {
                            "xpath": "/TargetEndpoint/HTTPTargetConnection/URL",
                            "value": "http://email.pcf.cloud-core.aig.net/v1/cm"
                        }
                    ]
                }
            ]
        }
    ]
}