<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Carrier Target endpoint">
    <Description/>
    <FaultRules>
        <FaultRule name="unauthorized_error">
            <Step>
                <Name>JS.FaultHandler</Name>
                <Condition>response.status.code != "500"</Condition>
            </Step>
            <Step>
                <Name>XTJ.ConvertToJson</Name>
                <Condition>response.status.code = "500"</Condition>
            </Step>
            <Step>
                <Name>JS.CreateResponse</Name>
                <Condition>response.status.code = "500"</Condition>
            </Step>
        </FaultRule>
    </FaultRules>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>JS.RewriteUrl</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>XTJ.ConvertToJson</Name>
            </Step>
            <Step>
                <Name>JS.CreateResponse</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <!--Property name="success.codes">1xx,2xx,3xx,4xx,5xx</Property>
        </Properties-->
        <URL>https://freedom.dev.travelguard.net/listservice/carrierwebservice.svc</URL>
    </HTTPTargetConnection>
</TargetEndpoint>