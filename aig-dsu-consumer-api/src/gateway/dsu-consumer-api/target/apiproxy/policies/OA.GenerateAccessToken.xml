<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OA.GenerateAccessToken">
    <DisplayName>OA.GenerateAccessToken</DisplayName>
    <Operation>GenerateAccessToken</Operation>
    <!-- This is in millseconds -->
    <ExpiresIn ref="dsuTtl">150000</ExpiresIn>
    <RefreshTokenExpiresIn ref="refreshTtl">300000</RefreshTokenExpiresIn>
    <SupportedGrantTypes>
        <GrantType>password</GrantType>
    </SupportedGrantTypes>
    <GrantType>tokenGrantType</GrantType>
    <UserName>dsuSiteKey</UserName>
    <PassWord>dsuApiTicket</PassWord>
    <Attributes>
        <Attribute name="clientIP" ref="clientIp"/>
        <Attribute name="ttl" ref="dsuTtl"> 150000</Attribute>
        <Attribute name="refreshTtl"> 300000</Attribute>
        <Attribute name="dsuUuid" ref="dsuUuid"/>
        <Attribute name="dsuLegacyid" ref="dsuLegacyid"/>
        <Attribute name="dsuEmail" ref="dsuEmail"/>
        <Attribute name="dsuIdToken" ref="dsuSessionToken"/>
    </Attributes>
    <GenerateResponse enabled="true"/>
    <GenerateErrorResponse enabled="true"/>
</OAuthV2>