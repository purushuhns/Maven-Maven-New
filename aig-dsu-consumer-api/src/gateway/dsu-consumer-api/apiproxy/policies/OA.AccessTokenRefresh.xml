<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OA.AccessTokenRefresh">
    <DisplayName>OA.AccessTokenRefresh</DisplayName>
    <Operation>RefreshAccessToken</Operation>
    <GrantType>request.formparam.grant_type</GrantType>
    <ExpiresIn ref="oauthv2accesstoken.GOA.GetOAuthAttributes.accesstoken.ttl">150000</ExpiresIn>
    <RefreshTokenExpiresIn ref="oauthv2accesstoken.GOA.GetOAuthAttributes.accesstoken.refreshTtl">300000</RefreshTokenExpiresIn>
    <RefreshToken>request.formparam.refreshtoken</RefreshToken>
    <GenerateResponse enabled="true"/>
</OAuthV2>